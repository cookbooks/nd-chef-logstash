NameVirtualHost *:<%= node['logstash']['kibana']['http_port'] %>
<VirtualHost *:<%= node['logstash']['kibana']['http_port'] %>>
  ServerName <%= @server_name %>
	DocumentRoot "<%= @docroot %>"
	

	<Directory "<%= @docroot %>">
		DirectoryIndex index.php
		AllowOverride All
	</Directory>
</VirtualHost>
